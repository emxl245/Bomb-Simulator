Second_tab:AddSwitch("Auto Damage",function()
    AutoDmg = (not AutoDmg);
end)
    
while wait(.001) do
    if AutoDmg == true then
game:GetService("ReplicatedStorage").Events.Server.RequestQuestClaim:InvokeServer("Deal Damage")
    else
        RESULT = nil
    end
end
